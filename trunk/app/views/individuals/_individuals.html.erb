<% for individual in @individuals %>
  <tr id='individual_<%= individual.id %>'>
    <td><%=h individual.login %></td>
    <td><%=h individual.email %></td>
    <td><%=h individual.first_name %></td>
    <td><%=h individual.last_name %></td>
    <td><%=h individual.activated? %></td>
    <td><%=h individual.enabled %></td>
	
    <td><%= link_to 'View', individual %></td>
    <td><%= link_to 'Edit', edit_individual_path(individual) %></td>
    <td><%= link_to_remote 'Delete', :url => individual_path(individual), :confirm => 'Are you sure?', :method => :delete, :update => 'individuals' %></td>
  </tr>
<% end %>
